#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/E:/Blog/Blog/node_modules/.store/transliteration@1.6.6/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/E:/Blog/Blog/node_modules/.store/transliteration@1.6.6/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../transliteration@1.6.6/node_modules/transliteration/lib/bin/slugify" "$@"
else
  exec node  "$basedir/../../../../../transliteration@1.6.6/node_modules/transliteration/lib/bin/slugify" "$@"
fi
